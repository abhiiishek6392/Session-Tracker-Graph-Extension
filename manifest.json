{
    "manifest_version" : 3 ,
    "name" : "GitHub Session Tracker",
    "version" : "1.0",
    "description" : "Tracks your GitHub activity session and displays them in a graph.",
    "permissions" : ["storage", "tabs"], 
    "background" : {
        "service_worker" :"background.js"
    },
    "content_scripts" : [
        {
            "matches" : ["*://github.com/*"],
    "js" : ["content.js"] 
}
    ],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon" : {
            "16" : "icons/icon.png"
        }
    },
    "icons" : {
        "16" : "icons/icon.png"
    }
}
